#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char num[1000];
    int n='0';
    scanf("%s",num);
    while(n<='9')
    {
        int c=0;
        for(int i=0;num[i]!='\0';i++)
        {
            if (num[i]==n)
                 c++;
            
        }
        
        printf("%d ",c);
        n++;
        
    }
    

    return 0;
}
